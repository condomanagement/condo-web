  "scripts": {
    "build": "NODE_OPTIONS='--no-experimental-webstorage' webpack --node-env=production --mode=production",
    "watch": "NODE_OPTIONS='--no-experimental-webstorage' webpack --watch",
    "start": "NODE_OPTIONS='--no-experimental-webstorage' webpack serve --open",
    "test": "jest test --coverage",
    "eject": "react-scripts eject",
    "docs": "npx typedoc --options ./typedoc.json src; touch docs/.nojekyll",
    "publish-docs": "node ./scripts/gh-pages.js",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "test-watch": "npx jest --watchAll --coverage",
    "build-watch": "npx webpack -w --mode=production",
    "build-production": "npx webpack --mode=production",
    "build-development": "npx webpack --mode=development",
    "build-analyze": "npx webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "build-server": "npx webpack --mode=production --config ./webpack.server.config.js",
    "server": "npx nodemon ./server-dist/server.js",
    "dev": "npx webpack serve --config ./webpack.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
